{
  "env": {
    "browser": true
  },
  "extends": ["alloy", "alloy/react"],
  "globals": {
    "React": "writable",
    "process": true
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "react/jsx-fragments": "off",
    "no-undef": "warn"
  }
}
