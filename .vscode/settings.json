{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
  ],
  "css.fileExtensions": [
    "css",
    "scss",
    "javascriptreact"
  ],
  "auto-rename-tag.activationOnLanguage": ["javascript", "javascriptreact"],
  "eslint.packageManager": "pnpm"
}