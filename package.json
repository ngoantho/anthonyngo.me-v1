{
  "name": "anthonyngo.me",
  "version": "0.8.0",
  "description": "My personal portfolio/website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngoantho/portfolio.git"
  },
  "author": "Anthony Ngo <contact@anthonyngo.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngoantho/portfolio/issues"
  },
  "homepage": "https://github.com/ngoantho/portfolio#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix ."
    ]
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "style": "module"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:full": "next build && next export",
    "start": "next start",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "prettier --check \"src/**/*.js\""
  },
  "dependencies": {
    "@prefresh/next": "^1.4.1",
    "goober": "^2.0.15",
    "goober-autoprefixer": "^1.2.1",
    "gray-matter": "^4.0.2",
    "milligram": "^1.4.1",
    "next": "10.0.3",
    "next-plugin-preact": "^3.0.3",
    "next-seo": "^4.17.0",
    "preact": "^10.5.7",
    "preact-render-to-string": "^5.1.12",
    "react": "npm:@preact/compat@0.0.3",
    "react-dom": "npm:@preact/compat@0.0.3",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.1.2",
    "sass": "^1.29.0"
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.1"
  }
}
